{
  "name": "Wave Unread Navigator",
  "version": "1.2",
  "description": "Show gmail-like arrows listing if unread blips in an open wave are above or below.",
  "icons": { 
           "16": "icon16.png",
           "32": "icon32.png",
           "64": "icon64.png",
          "128": "icon128.png" },
  "content_scripts": [
    {
      "matches": ["https://wave.google.com/*"],
      "js": ["wavenavigation.user.js"]
    }
  ]
}
